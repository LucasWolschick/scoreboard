/*
    UF:
    inteiro:3
    add:1
    mul:3
    INST:
    lw: 1
    mul: 1
    sub: 1
    div: 1
    add: 1
*/


.data

    # tamanho do vetor
    9

    # vetor a ser ordenado
    10 9 8 5 3 2 45 0 1

    

.text
    # r1 vai armazenar o valor de i 
    # r2 vai armazenar o valor de key
    # r3 vai armazenar o valor de j 
    # r4 vai armazenar o valor de n
    # r5 vai armazenar o valor de X
    # r14 tera um valor constante 4
    
    addi r1, r0, 4
    addi r14, r0, 4

    lw r4, 0(r0)
    mul r4, r4, r14
    
    # Inicio do LOOP:

    # if r1 < r4
    beq r1, r4, 52 #ELSE

    # r2 = arr[r1]
    lw r2, 4(r1)

    # j = r1 - 1
    subi r3, r1, 4

    # while
    # r3 >= 0 && arr[r3] > r2

    blt r3, r0, 24 #Fim-While
    lw r5, 4(r3) # arr[r3]
    blt r5, r2, 16 #Fim-While

    # arr[r3+1] = arr[r3]
    lw r5, 4(r3)
    sw r5, 8(r3)

    # r3 = r3 - 1
    subi r3, r3, 4

    j 128
    # FIM-while: 

    # arr[r3+1] = r2
    sw r2, 8(r3)

    addi r1, r1, 4

    j 116
    #Fim-for

    exit







    

    
